{
	"successChange": {
		"description": "Password changed with success",
		"schema": {
			"type": "object",
			"properties": {
				"status": {
					"type": "boolean",
					"default": true
				},
				"message": {
					"type": "string",
					"example": "Your password has changed with success!"
				}
			}
		}
	},
	"expiredToken": {
		"description": "Token already expire, need request new reset password",
		"schema": {
			"type": "object",
			"properties": {
				"status": {
					"type": "boolean",
					"default": false
				},
				"message": {
					"type": "string",
					"example": "Token expired, please request  new reset token."
				}
			}
		}
	},
	"differentPasswords": {
		"description": "New password and confirm password not equals",
		"schema": {
			"type": "object",
			"properties": {
				"status": {
					"type": "boolean",
					"default": false
				},
				"message": {
					"type": "string",
					"example": "Passwords d'ont match."
				}
			}
		}
	}
}